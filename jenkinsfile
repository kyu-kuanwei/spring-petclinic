pipeline {
  agent any
  stages {
    stage("Build") {
      agent any
      steps {
        sh './mvnw clean package'
      }
    }
    stage("SonarQube analysis") {
      steps {
        withSonarQubeEnv('sonar_qube') {
          sh './mvnw sonar:sonar'
        }
      }
      post {
        success {
          archiveArtifacts 'target/*.jar'
        }
      }
    }
  }
}